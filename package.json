{
  "name": "unocss-preset-hbs",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.0.1",
  "description": "Hongbusi's unocss preset.",
  "author": "Hongbusi",
  "license": "MIT",
  "homepage": "https://github.com/Hongbusi/unocss-preset-hbs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hongbusi/unocss-preset-hbs.git"
  },
  "bugs": "https://github.com/Hongbusi/unocss-preset-hbs/issues",
  "keywords": [
    "unocss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "pnpm -C playground run dev",
    "build": "unbuild",
    "update": "tsx scripts/update-readme.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@hongbusi/eslint-config": "^0.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "fs-extra": "^11.1.0",
    "tsx": "^3.12.1",
    "typescript": "^4.8.3",
    "unbuild": "^1.0.1",
    "unocss": "^0.47.5"
  }
}
